 pipeline {
     agent any
 ​
     stages {
         stage('Pull') {
             steps {
                 echo 'pull code'
             }
         }
         stage('Build') {
             steps {
                 echo 'Build project'
             }
         }
         stage('publish') {
             steps {
                 echo 'publish project'
             }
         }
     }
 }